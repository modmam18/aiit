#include <stdio.h>
#include <stdlib.h>

int main() {

    //Gauss`sche Summenformel "primitiv errechnet"
    //https://de.wikipedia.org/wiki/Gau%C3%9Fsche_Summenformel//
    int s = 0;


    for (int i = 0; i < 100; i++) {
        s = s + i;
    }

    printf("S = %d\n", s);
    
    

    return 0;

}
